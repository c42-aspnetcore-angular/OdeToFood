@model  Restaurant

<section>
    <h3>@Model.Name</h3>
    <div>
        Cuisine: @Model.Cuisine
    </div>
    <div>
        <a href="home/details/@Model.Id">Details (Anchor tag)</a>
        @Html.ActionLink("Go (Html.ActionLink)", "Details", new { Id = @Model.Id })
        <a asp-action="Details" asp-route-id="@Model.Id">More (tag helper)</a>
        <a asp-page="/Restaurants/Edit" asp-route-id="@Model.Id">Edit (Razor Page)</a>
    </div>
</section>